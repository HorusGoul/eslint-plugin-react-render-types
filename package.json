{
  "name": "eslint-plugin-react-render-types",
  "version": "0.7.2",
  "description": "ESLint plugin that brings Flow's Render Types to TypeScript via JSDoc comments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./lsp": {
      "types": "./dist/language-service-plugin.d.ts",
      "import": "./dist/language-service-plugin.js",
      "require": "./dist/language-service-plugin.js"
    }
  },
  "files": [
    "dist",
    "lsp.js"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "react",
    "render-types",
    "jsdoc",
    "typescript"
  ],
  "author": "Horus Lugo <hola@horus.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HorusGoul/eslint-plugin-react-render-types.git"
  },
  "bugs": {
    "url": "https://github.com/HorusGoul/eslint-plugin-react-render-types/issues"
  },
  "homepage": "https://github.com/HorusGoul/eslint-plugin-react-render-types#readme",
  "packageManager": "pnpm@10.14.0",
  "peerDependencies": {
    "@typescript-eslint/parser": ">=8.0.0",
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.53.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@typescript-eslint/parser": "^8.53.1",
    "@typescript-eslint/rule-tester": "^8.53.1",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}